{
  "name": "@maxigalaxi/vue-status-indicator",
  "version": "0.9.9",
  "description": "A simple, customizable status indicator component for Vue 3",
  "author": "Maximilian Rittler",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vue",
    "vue3",
    "status",
    "indicator",
    "component",
    "ui",
    "status-indicator",
    "dot",
    "pulse"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxiGalaxi/vue-status-indicator.git"
  },
  "bugs": {
    "url": "https://github.com/MaxiGalaxi/vue-status-indicator/issues"
  },
  "homepage": "https://github.com/MaxiGalaxi/vue-status-indicator#readme",
  "main": "dist/status-indicator.umd.js",
  "module": "dist/status-indicator.es.js",
  "style": "dist/status-indicator.css",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/status-indicator.es.js",
      "require": "./dist/status-indicator.umd.js"
    },
    "./dist/status-indicator.css": "./dist/status-indicator.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
